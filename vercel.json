{
  "buildCommand": "yarn build",
  "outputDirectory": "build",
  "framework": "docusaurus-2",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
